local players = game:GetService("Players")
local localplayer = players.LocalPlayer
local camera = workspace.CurrentCamera

return {
	UNIVERSAL={
		BodyParts = {
			["TORSO"] = "UpperTorso",
			["HEAD"] = "Head",
		},
		Init = function(self)

		end,
		SetCFrame = function(self,CFrame)
			camera.CFrame = CFrame
		end,
		Valid = function(self)
			return true
		end,
		Translate = function(self,text)
			return self.BodyParts[text:upper()] or ""
		end,
		GetIgnorables = function(self)
			return {}
		end,
		GetEnemies = function(self)
			local enemies = {}
			for _,player in next, players:GetPlayers() do
				if (player.Character and (player.Neutral or player.Team ~= localplayer)) and player ~= localplayer and player.Character:FindFirstChild("Humanoid").Health>0 then
					enemies[#enemies+1] = {
						Instance = player.Character,
						Parent = player.Character.Parent,
						Ignore = player.Character,
					}
				end
			end
			return enemies
		end,
		GetLocalCharacter = function(self)
			return localplayer.Character
		end,
		GetLocalPosition = function(self)
			local character = self:GetLocalCharacter()
			if character then
				return character[self:Translate("HEAD")].Position
			end
		end,
		Tabs = {
			Silent = false,
			WeaponModding = false,
			Visuals = true,
		},
		Info = {
			Created = os.time(),
			GameName = game.Name,
			GameId = game.GameId,
			PlaceId = game.GameId,
			AntiCheatHash = "N/A",
			ClientHash = "N/A",
			GetClientHash = function() return "N/A" end,
			GetAntiCheatHash = function() return "N/A" end,
			__TYPE = "UNVIERSAL",
		},
	},
	[1168263273] = {
		BodyParts = {
			["TORSO"] = "Chest",
			["HEAD"] = "Head",
		},
		Init = function(self)
		end,
		SetCFrame = function(self,CFrame)
			camera = CFrame
		end,
		Valid = function(self)
			return self:GetLocalCharacter() and true or false
		end,
		Translate = function(self,text)
			return self.BodyParts[text:upper()] or ""
		end,
		GetIgnorables = function(self)
			local weapon
			for _,descendant in pairs(workspace:GetChildren()) do
				if (descendant:IsA("Model")) and descendant:FindFirstChild("AnimationController") then
					weapon=descendant
					break
				end
			end
			return {unpack(workspace.Ragdolls:GetChildren()),unpack(workspace.Projectiles:GetChildren()),weapon}
		end,
		GetEnemies = function(self)
			local enemies = {}
			for _,character in pairs(workspace.Characters:GetChildren()) do
		        enemies[#enemies+1] = {
					Instance = character,
					Parent = character.Parent,
					Ignore = character,
				}
			end
			return enemies
		end,
		GetLocalCharacter = function(self)
			return true
		end,
		GetLocalPosition = function(self)
			local character = self:GetLocalCharacter()
			if character then
				return character[self:Translate("HEAD")].Position
			end
		end,
		Tabs = {
			Silent = false,
			WeaponModding = true,
			Visuals = true,
		},
		Info = {
			Created = 1635872321,
			GameName = "Bad Buisness",
			GameId = 1168263273,
			PlaceId = 3233893879,
			AntiCheatHash = "N/A",
			ClientHash = "N/A",
			GetClientHash = function() return "N/A" end,
			GetAntiCheatHash = function() return "N/A" end,
			__TYPE = "API",
		},
	},
