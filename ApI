		GetEnemies = function(self)
			for i,v in pairs(game.Players:GetPlayers()) do
                local enemies = {}
                local character = workspace.MapFolder.Players:FindFirstChild(v.Name)
                if character then
                    enemies[#enemies+1] = character
                end
                return enemies
            end
		end,
		GetLocalCharacter = function(self)
			return localplayer.Character
		end,
